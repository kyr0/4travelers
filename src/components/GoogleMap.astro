---
export interface Props {
    mapUrl: string;
    class?: string;
}
const { mapUrl, class: className } = Astro.props
---
<style>
    [data-astro-cookie-consent="google-maps"] {
        border-radius: 0.5rem;
    }
</style>
<center>
    <iframe consent-src={mapUrl} data-astro-cookie-consent="google-maps" width="100%" height="300" style="border:0;" allowfullscreen="" class={className} loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
</center>