---
import { markdownify } from "@lib/utils/textConverter";

const { page } = Astro.props;
const { title } = page.data;
const { Content } = await page.render();
---

<section class="section">
  <div class="container">
    <h1 set:html={markdownify(title)} class="h2 page-heading" />
    <div class="content">
      <Content />
    </div>
  </div>
</section>
